{
    "rewrites": [
        {
            "source": "/admin",
            "destination": "https://x-autoup.vercel.app/admin.html"
        },
        {
            "source": "/apply",
            "destination": "https://x-autoup.vercel.app/apply.html"
        },
        {
            "source": "/api/(.*)",
            "destination": "https://x-autoup.vercel.app/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm ci",
    "framework": "vite"
}