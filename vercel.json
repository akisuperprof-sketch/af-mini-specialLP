{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://x-autoup.vercel.app/api/:path*"
        },
        {
            "source": "/(dental|hayfever|pet|3dprinter)(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/admin",
            "destination": "/admin.html"
        },
        {
            "source": "/apply",
            "destination": "/apply.html"
        },
        {
            "source": "/((?!index\\.html$|admin$|apply$).*)",
            "destination": "/index.html"
        }
    ],
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite"
}